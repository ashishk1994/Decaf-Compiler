class Progra
{
	int main()
	{
		int i,j,arr[10],low,high,mid,val;
		low=0;
		high=10;
		for i=1,10000
		{
			mid=(low+high)/2;
			if(arr[mid]==val)
			{
				break;
			}
			if(val>arr[mid])
			{
				low=mid+1;
			}
			if(val<arr[mid])
			{
				high=mid-1;
			}
			if(low>high)
			{
				break;
			}
		}
	}
}
